{
  "name": "landscape-app",
  "version": "1.0.0",
  "description": "Provides an interface for landscape architects to communicate with property management firms and landscapers",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www && npm run build",
    "client": "cd client && npm run start",
    "server": "nodemon ./bin/www",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "homepage": "https://immense-coast-86387.herokuapp.com/",
  "engines": {
    "node": "8.9.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/killianmh/landscape-app.git"
  },
  "keywords": [
    "landscape",
    "architecture",
    "property",
    "management"
  ],
  "author": "Matt Killian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/killianmh/landscape-app/issues"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.5.3",
    "passport": "^0.4.0",
    "path": "^0.12.7",
    "react-router-dom": "^4.3.1",
    "sequelize": "^4.38.0"
  }
}
